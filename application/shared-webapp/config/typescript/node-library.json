{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node JS Library",
  "extends": "./base.json",
  "compilerOptions": {
    "lib": ["ES2023"],
    "noEmit": false,
    "module": "Preserve",
    "moduleResolution": "Bundler",
    "useDefineForClassFields": true,
    "allowSyntheticDefaultImports": true,
    "declaration": true
  }
}
