<?xml version="1.0" encoding="utf-8"?>
<Project>
  <PropertyGroup>
    <PackageJsonName Condition="$(PackageJsonName) == ''">account-management-webapp</PackageJsonName>
    <PackageJsonVersion Condition="$(PackageJsonVersion) == ''">1.0.0</PackageJsonVersion>
    <PackageJsonPrivate Condition="$(PackageJsonPrivate) == ''">true</PackageJsonPrivate>
    <PackageJsonScriptsDev Condition="$(PackageJsonScriptsDev) == ''">rsbuild dev</PackageJsonScriptsDev>
    <PackageJsonScriptsDevSetup Condition="$(PackageJsonScriptsDevSetup) == ''">npm run update-translations &amp;&amp; npm run swagger</PackageJsonScriptsDevSetup>
    <PackageJsonScriptsBuild Condition="$(PackageJsonScriptsBuild) == ''">npm run update-translations &amp;&amp; npm run swagger &amp;&amp; rsbuild build</PackageJsonScriptsBuild>
    <PackageJsonScriptsUpdateTranslations Condition="$(PackageJsonScriptsUpdateTranslations) == ''">lingui extract &amp;&amp; lingui compile --typescript</PackageJsonScriptsUpdateTranslations>
    <PackageJsonScriptsCheck Condition="$(PackageJsonScriptsCheck) == ''">biome check</PackageJsonScriptsCheck>
    <PackageJsonScriptsLint Condition="$(PackageJsonScriptsLint) == ''">biome lint --write</PackageJsonScriptsLint>
    <PackageJsonScriptsFormat Condition="$(PackageJsonScriptsFormat) == ''">biome format --write</PackageJsonScriptsFormat>
    <PackageJsonScriptsSwagger Condition="$(PackageJsonScriptsSwagger) == ''">openapi-typescript shared/lib/api/AccountManagement.Api.json -o shared/lib/api/api.generated.d.ts --properties-required-by-default -t --enum --alphabetize</PackageJsonScriptsSwagger>
    <PackageJsonScriptsPublish Condition="$(PackageJsonScriptsPublish) == ''">npm run build &amp;&amp; mkdir -p ../Api/publish/WebApp/dist/ &amp;&amp; rsync -r ./dist/ ../Api/publish/WebApp/dist/</PackageJsonScriptsPublish>
    <PackageJsonDevdependenciesRepoBuild Condition="$(PackageJsonDevdependenciesRepoBuild) == ''">*</PackageJsonDevdependenciesRepoBuild>
    <PackageJsonDevdependenciesRepoConfig Condition="$(PackageJsonDevdependenciesRepoConfig) == ''">*</PackageJsonDevdependenciesRepoConfig>
    <PackageJsonDevdependenciesRepoInfrastructure Condition="$(PackageJsonDevdependenciesRepoInfrastructure) == ''">*</PackageJsonDevdependenciesRepoInfrastructure>
    <PackageJsonDevdependenciesRepoUi Condition="$(PackageJsonDevdependenciesRepoUi) == ''">*</PackageJsonDevdependenciesRepoUi>
  </PropertyGroup>
</Project>